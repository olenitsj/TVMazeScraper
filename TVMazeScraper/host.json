{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }

    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:30",
      "visibilityTimeout": "00:01:00",
      "batchSize": 1,
      "maxDequeueCount": 5,
      "newBatchThreshold": 0
    }
  }
}